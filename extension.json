{
	"name": "PageViewInfo",
	"license-name": "GPL-3.0+",
	"author": [
		"Kunal Mehta"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:PageViewInfo",
	"descriptionmsg": "pvi-desc",
	"Hooks": {
		"InfoAction": [
			"MediaWiki\\Extensions\\PageViewInfo\\Hooks::onInfoAction"
		]
	},
	"AutoloadClasses": {
		"MediaWiki\\Extensions\\PageViewInfo\\Hooks": "includes/Hooks.php",
		"MediaWiki\\Extensions\\PageViewInfo\\PageViewService": "includes/PageViewService.php",
		"MediaWiki\\Extensions\\PageViewInfo\\WikimediaPageViewService": "includes/WikimediaPageViewService.php"
	},
	"MessagesDirs": {
		"PageViewInfo": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.pageviewinfo": {
			"scripts": [
				"ext.pageviewinfo.js"
			],
			"messages": [
				"pvi-close",
				"pvi-range"
			],
			"dependencies": [
				"ext.graph.vega2",
				"oojs-ui"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "PageViewInfo/resources"
	},
	"ConfigRegistry": {
		"PageViewInfo": "GlobalVarConfig::newInstance"
	},
	"ServiceWiringFiles": [
		"includes/ServiceWiring.php"
	],
	"config": {
		"PageViewInfoWikimediaEndpoint": "https://wikimedia.org/api/rest_v1",
		"PageViewInfoWikimediaDomain": false,
		"PageViewInfoWikimediaRequestLimit": 5
	},
	"manifest_version": 1
}
