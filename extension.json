{
	"name": "PageViewInfo",
	"license-name": "GPL-3.0+",
	"author": [
		"Kunal Mehta"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:PageViewInfo",
	"descriptionmsg": "pvi-desc",
	"Hooks": {
		"InfoAction": [
			"PageViewInfo\\Hooks::onInfoAction"
		]
	},
	"AutoloadClasses": {
		"PageViewInfo\\Hooks": "includes/PageViewInfo.hooks.php"
	},
	"MessagesDirs": {
		"PageViewInfo": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.pageviewinfo": {
			"scripts": [
				"ext.pageviewinfo.js"
			],
			"messages": [
				"pvi-close",
				"pvi-range"
			],
			"dependencies": [
				"ext.graph.vega2",
				"oojs-ui"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "PageViewInfo/resources"
	},
	"config": {
		"PageViewInfoEndpoint": "https://wikimedia.org/api/rest_v1/metrics/pageviews",
		"PageViewInfoDomain": false
	},
	"manifest_version": 1
}
